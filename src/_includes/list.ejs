<%
  const posts = items
  posts.forEach(item => {
%>
  <article>
    <a href="<%= item.url %>" class="permalink"><h3><%= item.data.title %></h3></a>
    <p><%= item.data.summary %></p>
    <section class="meta-wrap">
      <time datetime="<%= item.date.toUTCString() %>"><span aria-label="Posted on">&tau;</span><%= item.date.toISOString().substring(0, 10) %></time>
      <p class="tag"><span aria-hidden="true">#</span><a href="/tags/<%= item.data.tags %>" itemprop="about"><%= item.data.tags %></a></p>
    </section>
  </article>
<% }) %>