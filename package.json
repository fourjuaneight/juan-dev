{
  "name": "juan-dev",
  "version": "5.0.0",
  "description": "Freelance Front-End Developer, automation nerd, and Florida man.",
  "scripts": {
    "build": "gatsby build || gatsby build",
    "build:production": "npm run build && npm run js:sw",
    "clean:node": "rm -rf node_modules",
    "clean": "gatsby clean",
    "ci": "start-server-and-test http://localhost:8000",
    "cy:headless": "start-server-and-test start http://localhost:8000 cy:run",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.js",
    "jest:coverage": "jest --coverage",
    "jest:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "jest:watch": "jest --watch",
    "js:sw": "node cache-assets.js",
    "lint": "eslint --fix src/**/*.js",
    "serve": "gatsby serve",
    "start": "gatsby develop",
    "test:e2e": "is-ci \"cy:headless\" \"cy:open\"",
    "test:unit": "is-ci \"jest:coverage\" \"jest:watch\"",
    "test": "npm run test:e2e && npm run test:unit"
  },
  "lint-staged": {
    "*.+(js|css|ms)": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    ">3%",
    "not dead",
    "defaults",
    "safari >= 10.1",
    "ios >= 10.3",
    "ie >= 11"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://fourjuaneight@github.com/fourjuaneight/juan-dev.git"
  },
  "author": "Juan Villela <juan@villela.co>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fourjuaneight/juan-dev/issues"
  },
  "homepage": "https://github.com/fourjuaneight/juan-dev#readme",
