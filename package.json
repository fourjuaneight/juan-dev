{
  "name": "juan-dev",
  "version": "4.2.2",
  "description": "Freelance Front-End Developer, automation nerd, and Florida man.",
  "scripts": {
    "build": "gatsby build || gatsby build",
    "build:production": "npm run js:bg && npm run build && npm run js:sw",
    "clean:node": "rm -rf node_modules",
    "clean": "gatsby clean",
    "ci": "start-server-and-test http://localhost:8000",
    "cy:headless": "start-server-and-test start http://localhost:8000 cy:run",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.js",
    "jest:coverage": "jest --coverage",
    "jest:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "jest:watch": "jest --watch",
    "js:bg": "node canvas-bg.js",
    "js:sw": "node cache-assets.js",
    "lint": "eslint --fix src/**/*.js",
    "serve": "gatsby serve",
    "start": "npm run js:bg && gatsby develop",
    "test:e2e": "is-ci \"cy:headless\" \"cy:open\"",
    "test:unit": "is-ci \"jest:coverage\" \"jest:watch\"",
    "test": "npm run test:e2e && npm run test:unit"
  },
  "lint-staged": {
    "*.+(js|css|ms)": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    ">3%",
    "not dead",
    "defaults",
    "safari >= 10.1",
    "ios >= 10.3",
    "ie >= 11"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://fourjuaneight@github.com/fourjuaneight/juan-dev.git"
  },
  "author": "Juan Villela <juan@villela.co>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fourjuaneight/juan-dev/issues"
  },
  "homepage": "https://github.com/fourjuaneight/juan-dev#readme",
  "dependencies": {
    "@reach/skip-nav": "^0.8.2",
    "@svgr/webpack": "^5.1.0",
    "axios": "^0.19.2",
    "canvas": "^2.6.1",
    "classnames": "^2.2.6",
    "css-loader": "^3.2.0",
    "dotenv": "^8.2.0",
    "gatsby": "^2.19.16",
    "gatsby-cli": "^2.8.29",
    "gatsby-image": "^2.2.40",
    "gatsby-plugin-feed": "^2.3.27",
    "gatsby-plugin-manifest": "^2.2.23",
    "gatsby-plugin-netlify": "^2.1.31",
    "gatsby-plugin-preact": "^3.1.26",
    "gatsby-plugin-react-helmet": "^3.1.13",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-plugin-sitemap": "^2.2.19",
    "gatsby-plugin-svgr": "^2.0.2",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-footnotes": "^0.0.7",
    "gatsby-remark-images": "^3.1.44",
    "gatsby-remark-prismjs": "^3.3.23",
    "gatsby-remark-smartypants": "^2.1.14",
    "gatsby-source-filesystem": "^2.1.33",
    "gatsby-source-graphql": "^2.1.30",
    "gatsby-transformer-remark": "^2.6.30",
    "gatsby-transformer-sharp": "^2.2.23",
    "glob": "^7.1.6",
    "graphiql": "^0.17.5",
    "graphql": "^14.5.8",
    "graphql-tools": "^4.0.5",
    "graphql-tools-fork": "^8.5.2",
    "mutationobserver-shim": "^0.3.3",
    "preact": "^10.3.1",
    "preact-render-to-string": "^5.0.6",
    "prismjs": "^1.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-hook-form": "^4.9.3",
    "react-html-parser": "^2.0.2",
    "react-spring": "^8.0.27",
    "replace-in-file": "^5.0.2",
    "sanitize-html": "^1.20.1"
  },
  "devDependencies": {
    "@testing-library/cypress": "^5.3.0",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "axe-core": "^3.5.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "cypress": "^4.0.1",
    "cypress-axe": "^0.5.3",
    "eslint": "^6.5.1",
    "eslint-config-jcv": "^0.1.1",
    "eslint-plugin-cypress": "^2.9.0",
    "husky": "^4.2.2",
    "identity-obj-proxy": "^3.0.0",
    "is-ci-cli": "^2.0.0",
    "jest": "^25.1.0",
    "jest-in-case": "^1.0.2",
    "lint-staged": "^10.0.7",
    "prettier": "^1.18.2",
    "start-server-and-test": "^1.10.8"
  }
}
