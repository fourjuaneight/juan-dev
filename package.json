{
  "name": "juan-dev",
  "version": "3.0.0",
  "description": "Front-End Developer, automation nerd, and astronomy enthusiast.",
  "scripts": {
    "build": "webpack && npm run production",
    "build:dev": "webpack && npm run development",
    "clean": "rm -rf dist",
    "clean:node": "rm -rf node_modules",
    "development": "cross-env ELEVENTY_ENV=development npx eleventy",
    "production": "cross-env ELEVENTY_ENV=production npx eleventy",
    "local": "npm run clean && cross-env ELEVENTY_ENV=local npx eleventy --serve",
    "start": "webpack && npm run local"
  },
  "repository": {
    "type": "git",
    "url": "git+https://fourjuaneight@github.com/fourjuaneight/juan-dev.git"
  },
  "author": "Juan Villela",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fourjuaneight/juan-dev/issues"
  },
  "homepage": "https://github.com/fourjuaneight/juan-dev#readme",
  "engines": {
    "node": "10.15.1"
  },
  "resolutions": {
    "natives": "1.1.3"
  },
  "browserslist": [
    "> 3%",
    "defaults",
    "safari >= 10",
    "ios >= 10"
  ],
  "devDependencies": {
    "@11ty/eleventy": "^0.8.3",
    "@11ty/eleventy-plugin-rss": "^1.0.6",
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.3",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
    "extract-loader": "^3.1.0",
    "file-loader": "^4.0.0",
    "html-minifier": "^4.0.0",
    "json-to-scss": "^1.3.1",
    "markdown-it-link-attributes": "^2.1.0",
    "node-sass": "^4.12.0",
    "postcss-cli": "^6.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "sass-loader": "^7.1.0",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2"
  }
}
